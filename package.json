{
  "name": "tarotapp",
  "version": "1.0.0",
  "description": "Simply tarot app.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node backend/index.js",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "server": "node backend/index.js",
    "dev": "nodemon backend/index.js",
    "frontend": "cd frontend && npm run dev",
    "dew": "concurrently  \"npm run server\" \"npm run frontend\"",
    "preview": "cd frontend && vite preview"
  },
  "keywords": [
    "tarotcard",
    "tarot"
  ],
  "author": "Aleksandra Åšnigucka",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "concurrently": "^9.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "react-hot-toast": "^2.4.1",
    "sharp": "^0.33.5",
    "util": "^0.12.5",
    "validator": "^13.12.0",
    "vite": "^5.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "sucrase": "^3.35.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
